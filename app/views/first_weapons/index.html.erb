<p id="notice"><%= notice %></p>

<h1>First Weapons</h1>

<table>
  <thead>
    <tr>
      <th>Ranged</th>
      <th>Melee</th>
      <th>Primaryaddedmeleestr</th>
      <th>Secondaryaddedmeleestr</th>
      <th>Primarymeleerend</th>
      <th>Secondarymeleerend</th>
      <th>Firstshootattacks</th>
      <th>Firstshootattacksd3</th>
      <th>Firstshootattacksd6</th>
      <th>Secondshootattacks</th>
      <th>Secondshootattacksd3</th>
      <th>Secondshootattacksd6</th>
      <th>Thirdshootattacks</th>
      <th>Fourthshootattacks</th>
      <th>Firstrange</th>
      <th>Secondrange</th>
      <th>Thirdrange</th>
      <th>Fourthrange</th>
      <th>Firstshootattacks</th>
      <th>Secondshootattacks</th>
      <th>Thirdshootattacks</th>
      <th>Fourthshootattacks</th>
      <th>Firstshootattacksdamaged3</th>
      <th>Firstshootattacksdamaged6</th>
      <th>Secondshootattacksdamaged3</th>
      <th>Secondshootattacksdamaged6</th>
      <th>Points</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @first_weapons.each do |first_weapon| %>
      <tr>
        <td><%= first_weapon.ranged %></td>
        <td><%= first_weapon.melee %></td>
        <td><%= first_weapon.primaryAddedMeleeStr %></td>
        <td><%= first_weapon.secondaryAddedMeleeStr %></td>
        <td><%= first_weapon.primaryMeleeRend %></td>
        <td><%= first_weapon.secondaryMeleeRend %></td>
        <td><%= first_weapon.firstShootAttacks %></td>
        <td><%= first_weapon.firstShootAttacksD3 %></td>
        <td><%= first_weapon.firstShootAttacksD6 %></td>
        <td><%= first_weapon.secondShootAttacks %></td>
        <td><%= first_weapon.secondShootAttacksD3 %></td>
        <td><%= first_weapon.secondShootAttacksD6 %></td>
        <td><%= first_weapon.thirdShootAttacks %></td>
        <td><%= first_weapon.fourthShootAttacks %></td>
        <td><%= first_weapon.firstRange %></td>
        <td><%= first_weapon.secondRange %></td>
        <td><%= first_weapon.thirdRange %></td>
        <td><%= first_weapon.fourthRange %></td>
        <td><%= first_weapon.firstShootAttacks %></td>
        <td><%= first_weapon.secondShootAttacks %></td>
        <td><%= first_weapon.thirdShootAttacks %></td>
        <td><%= first_weapon.fourthShootAttacks %></td>
        <td><%= first_weapon.firstShootAttacksDamageD3 %></td>
        <td><%= first_weapon.firstShootAttacksDamageD6 %></td>
        <td><%= first_weapon.secondShootAttacksDamageD3 %></td>
        <td><%= first_weapon.secondShootAttacksDamageD6 %></td>
        <td><%= first_weapon.points %></td>
        <td><%= link_to 'Show', first_weapon %></td>
        <td><%= link_to 'Edit', edit_first_weapon_path(first_weapon) %></td>
        <td><%= link_to 'Destroy', first_weapon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

